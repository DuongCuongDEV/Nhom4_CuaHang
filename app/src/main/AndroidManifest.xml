<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fpoly.quanly">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quanly"
        tools:targetApi="31">
        <activity android:name=".Activity.Shaper"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Loading"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Login.SignUp"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.HoaDon"
            android:exported="false" />
        <activity
            android:name=".Activity.QuanLySanPham"
            android:exported="false" />
        <activity
            android:name=".Activity.ThemSanPham"
            android:exported="false" />
        <activity
            android:name=".Activity.ThongKe"
            android:exported="false" />
        <activity
            android:name=".Activity.Home"
            android:exported="false" />
        <activity
            android:name=".Login.SignIn"
            android:exported="true">
        </activity>
    </application>

</manifest>